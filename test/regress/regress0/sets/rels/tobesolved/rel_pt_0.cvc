% EXPECT: sat
OPTION "logic" "ALL_SUPPORTED";
IntPair: TYPE = [INT, INT];
IntTup: TYPE = [INT, INT, INT, INT];
x : SET OF IntPair;
y : SET OF IntPair;
r : SET OF IntPair;
w : SET OF IntTup;

z : IntPair;
ASSERT z = (1,3);
zt : IntPair;
ASSERT zt = (2,1);
v : IntTup;
ASSERT v = (1,2,2,1);

ASSERT zt IS_IN y;
ASSERT v IS_IN (x PRODUCT y);
ASSERT (4, 4, 5, 5) IS_IN w;
ASSERT  z IS_IN x;

ASSERT w = (x PRODUCT y);
ASSERT NOT (4,4) IS_IN x;

CHECKSAT;
