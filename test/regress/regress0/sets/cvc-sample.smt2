OPTION "logic" "ALL_SUPPORTED";
SetInt : TYPE = SET OF INT;
PUSH;
a : SET OF INT;
b : SET OF INT;
c : SET OF INT;
e : INT;
ASSERT a = {5};
ASSERT c = (a | b);
ASSERT NOT(c = (a & b));
ASSERT c = (a - b);
ASSERT a <= b;
ASSERT e IN c;
ASSERT e IN a;
ASSERT e IN (a & b);
CHECKSAT TRUE;
POP;
PUSH;
x : SET OF INT;
y : SET OF INT;
z : SET OF INT;
ASSERT x = y;
ASSERT NOT((x | z) = (y | z));
CHECKSAT TRUE;
POP;
PUSH;
x : SET OF INT;
y : SET OF INT;
e1 : INT;
e2 : INT;
ASSERT x = y;
ASSERT e1 = e2;
ASSERT e1 IN x;
ASSERT NOT(e2 IN y);
CHECKSAT TRUE;
POP;
PUSH;
x : SET OF INT;
y : SET OF INT;
z : SET OF INT;
e1 : INT;
e2 : INT;
ASSERT x = y;
ASSERT e1 = e2;
ASSERT e1 IN (x | z);
ASSERT NOT(e2 IN (y | z));
CHECKSAT TRUE;
