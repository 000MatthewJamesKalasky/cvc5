a, b, c: BOOLEAN;

% EXPECT: VALID
QUERY (a XOR b) <=> (NOT a AND b) OR (NOT b AND a);

% EXPECT: INVALID
QUERY NOT c AND b;

% EXPECT: VALID
QUERY (IF c THEN a ELSE b ENDIF) <=> ((c AND a) OR (NOT c AND b));

% EXPECT: VALID
QUERY (a => b) <=> (NOT a OR b);

% EXPECT: VALID
QUERY TRUE XOR FALSE;
 
